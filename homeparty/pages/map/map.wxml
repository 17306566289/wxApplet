<map id="map" longitude="{{longitude}}"
 latitude="{{latitude}}"   wx:if="{{map}}"
  controls="{{controls}}" scale="{{scale}}"
  bindcontroltap="controltap" markers="{{markers}}"
   bindmarkertap="markertap" polyline="{{polyline}}"
    bindregionchange="regionchange"
     show-location style="width: 100%; height:1205rpx;">
</map>